{
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontSize": 15,
    "files.exclude": {
        "**/.idea": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/*.pyc": {
            "when": "$(basename).py"
        },
        "**/__pycache__": true
    },
    "terminal.integrated.fontSize": 15,
    "php.validate.executablePath": "php",
    "go.formatTool": "goimports",
    "editor.suggestSelection": "first",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.explorerKind": "external",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "[go]": {
        "editor.formatOnSave": true
    },
    "latex-workshop.view.pdf.viewer": "tab",
    "go.useLanguageServer": true,
    "C_Cpp.updateChannel": "Insiders",
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "javascript.format.enable": false,
    "php-cs-fixer.executable": "~/.composer/vendor/bin/php-cs-fixer",
    "php-cs-fixer.pathMode": "override",
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path": "/Users/joschavez/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "xml.format.splitAttributes": true,
    "editor.formatOnSave": true,
    "namespaceResolver.sortOnSave": true,
    "namespaceResolver.sortNatural": true,
    "namespaceResolver.showMessageOnStatusBar": true,
    "symfony-vscode.enableFileWatching": false,
    "[java]": {
        "editor.defaultFormatter": "mngrm3a.vscode-google-java-formatter"
    },
    "editor.minimap.enabled": false,
    "editor.accessibilitySupport": "off",
    "go.toolsManagement.autoUpdate": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "redhat.telemetry.enabled": true,
    "editor.inlineSuggest.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "go.executable.path": "/opt/homebrew/bin/go",
    "yaml.schemas": {
        "https://github.com/jcchavezs/envoy-config-schema/releases/download/v1.21.0/v3_Bootstrap.json": "envoy-config.yaml"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "go.testTimeout": "300s",
    "window.commandCenter": false,
    "window.zoomLevel": 2,
    "security.workspace.trust.untrustedFiles": "open"
}