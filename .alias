alias ea='vim ~/.dotfiles/.alias'
alias phpfix='php-cs-fixer fix'
alias ws='cd ~/Workspace'

alias vsh='vagrant ssh'
alias vup='vagrant up'
alias vspnd='vagrant suspend'

alias src='cd ~/Workspace/source'
alias gsrc='cd ~/Workspace/gource'
alias autoenv='export $(cat ./.env.local | xargs) && echo "Autloading .env"'

alias gs='git status -s'
alias g='git'
alias gco='git checkout'
alias gcom='git checkout master'
alias gdc='git diff --cached'
alias gfa='git fetch --all'
alias gcm='git commit -m '
alias gpo='git pull origin'
alias gpom='git pull origin master'
alias gpom='git pull origin master'
alias gpro='git pull --rebase origin'
alias gprom='git pull --rebase origin master'
alias grh1s='git reset HEAD~1 --soft'
alias grh='git reset HEAD'
alias gg='git log --pretty="format:%C(yellow)%h% %Cblue %ar %Cgreen %aE %Creset %s%C(auto,red)% gD% D"'
alias gcp='git cherry-pick'
alias gpr="git branch --merged | grep -v master | grep -v '*' > .merged-branches.tmp && vim .merged-branches.tmp && xargs git branch -d < .merged-branches.tmp; rm .merged-branches.tmp"
alias go='grc go'
alias gcl='git clean -f -d'
alias gram='git stash && gco master && git pull origin master && gco - && git pull --rebase origin master && git stash pop'

